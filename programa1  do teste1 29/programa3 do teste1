/*programa3 da prova*/
#include <stdio.h>
void main(void){
        int notas[5];
        char i , j , nota , soma, cont;
        for (i=0; i<5; i++){
                printf("digite a nota do aluno:\n");
                scanf("%hhd" , &nota);
                notas[i]=(nota);
                soma+=nota;
        }
        printf("media:%f\n" , soma/5.0);
        float media=soma/5.0;

        for (j=0; j<5 ; j++){
            if(notas[j]>media){
                    cont++;
            }
        }
        printf("numero de alunos acima da media:%d\n" , cont);
}

